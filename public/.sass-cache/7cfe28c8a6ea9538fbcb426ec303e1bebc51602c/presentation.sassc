h1,h2,h3,h4,h5,h6
  :font-family "Proxima Nova", "Helvetica Neue", "Helvetica", Arial, sans-serif
  :font-weight normal
h3
  :margin-bottom 0
h4
  :font-size 24px
  :font-size 2.4rem
h5
  :font-size 18px
  :font-size 1.8rem
h6
  :font-size 15px
  :font-size 1.5rem
  :line-height 1
p
  :line-height 20px
  :font-size 13px
  :font-size 1.3rem
.row
  :max-width 980px
strong
  :font-weight 600
.button.nice
  :background-image image-url("button-gloss.png")
.button.blue
  :background-color $blue
a
  :color $blue
  &:hover
    :color darken($blue, 10%)
.light-weight
  :font-weight 300 !important
.normal-weight
  :font-weight normal !important


/* ZURB bar */
#zurBar
  :background $black
  :border-bottom solid 4px $blue
  :padding 15px 0 13px 0
  h1, h2
    :margin-bottom 0
    :line-height 1
  h1
    :color $white
    :font-size 16px
    :font-size 1.6rem
    :font-weight 800
  h2
    :text-indent -99999px
    :display block
    :width 82px
    :height 14px
    :background image-url("by-zurb.png")

/* Grid BG */
#gridBg
  .row
    .columns
      :border-left dashed 1px lighten($black, 75%)
      :border-right dashed 1px lighten($black, 75%)
      :height 645px
      :width 4.085%
#gridBgShort
  :height 105px
  :position relative
  .row
    .columns
      :border-left dashed 1px lighten($black, 75%)
      :border-right dashed 1px lighten($black, 75%)
      :height 105px
      :width 4.085%
  div.white-fade
    :background image-url("white-fade.png") left bottom repeat-x
    :position absolute
    :top 0
    :left 0
    :width 100%
    :height 105px
    :z-index 20

/* Header */  
header
  :text-align center
  :position relative
  :z-index 100
  :top -645px
  :margin-top 40px
  h1
    :font-weight 300
    :font-size 76px
    :font-size 7.6rem
    :position relative
    :margin-bottom 0
    small
      :font-weight normal
      :font-size 32px
      :font-size 3.2rem
      :position absolute
      :top -8px
      :left 50%
      :margin-left -32px
  h2
    :font-weight 300
    :font-size 20px
    :font-size 2.0rem
    :margin-bottom 34px
    :padding 0 25%
  .button.nice
    :font-size 19px
    :font-size 1.9rem
    :background-position 0 -20px
    :padding-left 25px
    :padding-right 25px
    small
      :font-size 11px
      :font-size 1.1rem
      :font-weight 300
      :text-shadow none
  p
    :margin-top 8px
    a
      :margin-right 20px
      :font-size 12px
      :font-size 1.2rem
      &+a
        :margin-right 0
  img
    :margin-top 11px
    
/* Main Page */
#main
  :position relative
  :margin-top -740px
  :z-index 1
  :background-color #ededed
  +background-image(linear-gradient(top, #ededed, #ffffff 70%, #ffffff))
  :padding-top 115px
  :overflow hidden
  &:before
    :content ""
    :display block
    :background image-url("device-shadow.png") 0 0 no-repeat
    :width 1034px
    :height 140px
    :position absolute
    :top 0
    :left 50%
    :margin-left -517px

/* Update Bar */  
#updates
  :margin-bottom 60px
  div.two.columns
    +background-image(linear-gradient(top, #b7b7b7, #a6a6a6))
    :border solid 1px darken($white, 42%)
    :padding 12px 17px 15px 17px
    :width 96px
    h6
      :color $white
      :text-shadow 0 -1px 0 rgba(0,0,0,0.2)
      :font-weight 600
    time
      :color $white
      :text-shadow 0 -1px 0 rgba(0,0,0,0.12)
      :font-size 12px
      :font-size 1.2rem
      :margin-bottom 5px
      :display block
    .button.nice
      :color lighten($black, 30%)
      :font-weight 300
      :font-size 11px
      :font-size 1.1rem
      :border solid 1px darken($white, 40%)
      :padding 3px 20px 4px
  article
    :background $white
    :border-top solid 1px darken($white, 12%)
    :border-bottom solid 1px darken($white, 12%)
    :margin-left 0
    :padding 12px 17px 14px 2%
    dl
      :margin-bottom 4px
      dt
        :font-weight 600
        :font-size 14px
        :font-size 1.4rem
        :margin-bottom 4px
    a
      :font-size 11px
      :font-size 1.1rem
      :font-style italic
      :background image-url("octocat.png") 0 -1px no-repeat
      :padding-left 22px
      :position relative
      :left -2px
  aside
    :background $white
    :margin-left 0
    :padding 12px 17px 15px 2%
    :border solid 1px darken($white, 12%)
    :width 220px
    &>a
      :background image-url("icon-docs.png") 0 4px no-repeat
      :padding-left 30px
      :padding-top 5px
      :padding-bottom 5px
      :margin-bottom 5px
      :display block
      &+a
        :background image-url("icon-download.png") 0 4px no-repeat
        :margin-bottom 0
        :margin-right 0

/* Value Buckets Fool! */
#valueProps
