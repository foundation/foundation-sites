+fontFace("Proxima Nova", font-files("fonts/proximanova-bold-webfont.woff", woff, "fonts/proximanova-bold-webfont.ttf", truetype, "fonts/proximanova-bold-webfont.svg", svg), "fonts/proximanova-bold-webfont.eot", 800)

