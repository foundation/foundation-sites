    "touch": "^1.0.0",
    "vinyl": "^2.0.2",
    "vinyl-named": "^1.1.0",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "^2.6.1",
    "webpack-stream": "^3.2.0",
    "yargs": "^6.6.0"
