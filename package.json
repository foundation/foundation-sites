    "gulp-sourcemaps": "^2.6.0",
    "gulp-uglify": "^2.1.2",
    "gulp-zip": "^3.2.0",
    "inquirer": "^2.0.0",
    "is-empty-object": "^1.1.1",
    "js-yaml": "^3.8.4",
