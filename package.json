  "engines": {
    "node": ">=4.0",
    "npm": ">=2.14.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zurb/foundation-sites"
  },
  "bugs": {
    "url": "https://github.com/zurb/foundation-sites/issues",
    "email": "foundation@zurb.com"
  },
  "keywords": [
    "eyeglass-module",
    "handlebars-helper-rel",
    "handlebars-helper-slugify"
  ],
  "eyeglass": {
    "name": "foundation",
    "sassDir": "scss",
    "needs": "^0.8.0",
    "exports": false
  },
  "jspm": {
    "format": "global",
    "shim": {
      "dist/js/foundation": {
        "deps": [
          "jquery"
        ]
      }
    }
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  }
